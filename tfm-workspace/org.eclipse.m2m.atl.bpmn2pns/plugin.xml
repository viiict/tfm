<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.eclipse.m2m.atl.bpmn2pns.handlers.TransformHandler"
            commandId="org.eclipse.m2m.atl.bpmn2pns.commands.TransformCommand">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="true"
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <command
               commandId="org.eclipse.m2m.atl.bpmn2pns.commands.TransformCommand"
               label="PORFA FUNCIONA"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <iterate>
                  <test
                        property="org.eclipse.core.resources.extension"
                        value="bpmn">
                  </test>
               </iterate>
            </visibleWhen>
         </command>
      </menuContribution>
   </extension>

</plugin>
